[*.cs]

# CS4014: Так как этот вызов не ожидается, выполнение существующего метода продолжается до тех пор, пока вызов не будет завершен
dotnet_diagnostic.CS4014.severity = none

# CS8632: Аннотацию для ссылочных типов, допускающих значения NULL, следует использовать в коде только в контексте аннотаций "#nullable".
dotnet_diagnostic.CS8632.severity = silent
